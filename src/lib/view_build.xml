<?xml version="1.0" encoding="UTF-8"?>
<project name="TotemMVC-Core-Lib" default="make" basedir=".">
    <description>
        Makes the Totem.Code.phar library
    </description>
    <target name="init">
        <exec executable="to.phar">
            <arg value="-o Totem.View.phar" />
            <arg value="-s Totem/view_stub.php" />
            <arg value="-f SPL/SplClassLoader.php,Totem/View/ViewInterface.php,Totem/View/BaseView.php" />
        </exec>
    </target>
</project>