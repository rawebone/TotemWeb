<?xml version="1.0" encoding="UTF-8"?>
<project name="TotemMVC-Core-Lib" default="make" basedir=".">
    <description>
        Makes the Totem.Code.phar library
    </description>
    <target name="init">
        <exec executable="to.phar">
            <arg value="-o Totem.Model.phar" />
            <arg value="-s Totem/model_stub.php" />
            <arg value="-f SPL/SplClassLoader.php" />
        </exec>
    </target>
</project>